{
  "name": "sao-generator",
  "version": "2.0.0",
  "description": "Scaffold out an SAO generator",
  "files": [
    "saofile.js",
    "template"
  ],
  "repository": {
    "url": "brpaz/sao-generator",
    "type": "git"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "commit": "git cz"
  },
  "author": "Bruno Paz",
  "license": "MIT",
  "dependencies": {
    "sao": "^1.7.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.7",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "26.3.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
