{
  "name": "sao-generator",
  "version": "2.0.0",
  "description": "Scaffold out an SAO generator",
  "files": [
    "saofile.js",
    "template"
  ],
  "repository": {
    "url": "saojs/sao-generator",
    "type": "git"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "commit": "git cz"
  },
  "author": "Bruno Paz",
  "license": "MIT",
  "dependencies": {
    "sao": "^1.7.0"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "ava": "^3.6.0",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^26.1.0",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
